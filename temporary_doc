// Library implementation of bet_1st.cpp#include "bet_1st.h"table bet_1st(table &mytable){	float random;	random = ((rand()) / (RAND_MAX));		ranking(mytable.player[(mytable.currentplayer)]);		if (mytable.betsinroud == 1)	{		if (mytable.pot == (mytable.bet_level * 1.5))		{			switch (mytable.player[(mytable.currentplayer)].position)			{				case 9 :					if (mytable.player[(mytable.currentplayer)].rank.value <= 8)	// Rank 8 or better					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 8 :					if (mytable.player[(mytable.currentplayer)].rank.value <= 7)	// Rank 7 or better					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 7 :				case 6 :				case 5 :					if ((mytable.player[(mytable.currentplayer)].rank.value <= 3) ||						(mytable.player[(mytable.currentplayer)].special == 4))	// Rank 3 or better or case 4(AJ, KQ)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if ((mytable.player[(mytable.currentplayer)].rank.value >= 6) &&						(mytable.player[(mytable.currentplayer)].rank.kind == 1))	// Small pair					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 8)	// Rank 8 or better					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 4 :				case 3 :				case 2 :					if (mytable.player[(mytable.currentplayer)].special == 1)	// case 1(AA, KK, QQ, AK, AQ)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].special == 2)	// case 2(AKs, AQs, AJs, KQs)					{						if (random < (2 / 3))						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 4) // Rank of 4 or better					{						if (random < (1 / 3))						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}					}					if (mytable.player[(mytable.currentplayer)].rank.value == 5) // Rank of 5					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 1 :					if (mytable.player[(mytable.currentplayer)].rank.value <= 3)	// Rank 3 or better					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 8)	// Rank 8 or better					{						if (random < .7)						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'f';							return mytable;						}					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 0 :					mytable.player[(mytable.currentplayer)].action = 'r';					return mytable;					break;			}		}		else		{			switch (mytable.player[(mytable.currentplayer)].position)			{				case 9 :					if ((mytable.player[(mytable.currentplayer)].rank.kind == 1) &&						(mytable.player[(mytable.currentplayer)].rank.value >= 5) &&						(mytable.pot >= (mytable.bet_level * 5.5)))					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}				case 8 :					if (mytable.pot <= (mytable.bet_level * 3.5))					{						if (mytable.player[(mytable.currentplayer)].rank.value <= 6)						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						if (mytable.player[(mytable.currentplayer)].rank.value <= 7)						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'f';							return mytable;						}					}					else					{						if (mytable.player[(mytable.currentplayer)].rank.value <= 4)						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						if (mytable.player[(mytable.currentplayer)].rank.value <= 7)						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'f';							return mytable;						}					}					break;				case 7 :				case 6 :				case 5 :					if (mytable.player[(mytable.currentplayer)].rank.value <= 3)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 6)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 4 :				case 3 :				case 2 :					if (mytable.player[(mytable.currentplayer)].rank.value <= 2)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 4)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 1 :					if ((mytable.pot <= (mytable.bet_level * 3.5)) &&						(mytable.player[(mytable.currentplayer)].rank.value <= 3) &&						((mytable.player[((mytable.currentplayer + 7) % 10)].action == 'c') ||						 (mytable.player[((mytable.currentplayer + 8) % 10)].action == 'c')))					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if ((mytable.pot <= (mytable.bet_level * 5.5)) &&						(mytable.player[(mytable.currentplayer)].special == 4))					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 2)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 4)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}					break;				case 0 :					if (mytable.pot >= (mytable.bet_level * 4.5))					{						if (mytable.player[(mytable.currentplayer)].rank.value <= 2)						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						if (mytable.player[(mytable.currentplayer)].rank.value <= 4)						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'f';							return mytable;						}					}					else					{						if (mytable.player[(mytable.currentplayer)].rank.value <= 4)						{							mytable.player[(mytable.currentplayer)].action = 'r';							return mytable;						}						if (mytable.player[(mytable.currentplayer)].rank.value <= 6)						{							mytable.player[(mytable.currentplayer)].action = 'c';							return mytable;						}						else						{							mytable.player[(mytable.currentplayer)].action = 'f';							return mytable;						}					}					break;			}		}	}	if (mytable.betsinround == 2)	{		switch (mytable.player[(mytable.currentplayer)].position)		{			case 9 :			case 8 :				if ((mytable.player[(mytable.lastraiser)].position <= 4) &&					(mytable.player[(mytable.lastraiser)].position >= 0))				{					if (mytable.player[(mytable.currentplayer)].rank.value <= 2)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 4)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}				}				if ((mytable.player[(mytable.lastraiser)].position <= 7) &&					(mytable.player[(mytable.lastraiser)].position >= 5))				{					if (mytable.player[(mytable.currentplayer)].rank.value <= 3)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 5)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}				}				else				{					if (mytable.player[(mytable.currentplayer)].rank.value <= 4)					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					if (mytable.player[(mytable.currentplayer)].rank.value <= 6)					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'f';						return mytable;					}				}				break;			case 7 :			case 6 :			case 5 :				if ((mytable.player[(mytable.currentplayer)].special == 1) ||					(mytable.player[(mytable.currentplayer)].rank.value == 1))				{					mytable.player[(mytable.currentplayer)].action = 'r';					return mytable;				}				if (mytable.player[(mytable.currentplayer)].rank.value <= 3)				{					if (random < (1 / 3)))					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}				}				if (mytable.player[(mytable.currentplayer)].rank.value <= 5)				{					mytable.player[(mytable.currentplayer)].action = 'c';					return mytable;				}				else				{					mytable.player[(mytable.currentplayer)].action = 'f';					return mytable;				}				break;			case 4 :			case 3 :			case 2 :				if ((mytable.player[(mytable.currentplayer)].rank.kind == 2) &&					(mytable.player[(mytable.currentplayer)].rank.value <= 3))				{					if (random < (1 / 2))					{						mytable.player[(mytable.currentplayer)].action = 'r';						return mytable;					}					else					{						mytable.player[(mytable.currentplayer)].action = 'c';						return mytable;					}				}				if ((mytable.player[(mytable.lastraiser)].position < mytable.player[(mytable.currentplayer)].position) &&					(mytable.player[(mytable.currentplayer)].rank.value <= 3))				{					mytable.player[(mytable.currentplayer)].action = 'c';					return mytable;				}				if (mytable.player[(mytable.currentplayer)].rank.value <= 4)				{					mytable.player[(mytable.currentplayer)].action = 'c';					return mytable;				}				else				{					mytable.player[(mytable.currentplayer)].action = 'f';					return mytable;				}				break;